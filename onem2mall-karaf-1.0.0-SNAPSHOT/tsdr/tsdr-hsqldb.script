CREATE SCHEMA PUBLIC AUTHORIZATION DBA
CREATE MEMORY TABLE METRICS(KEYA BIGINT NOT NULL,KEYB BIGINT NOT NULL,TIME BIGINT NOT NULL,VALUE DOUBLE,PRIMARY KEY(KEYA,KEYB,TIME))
CREATE MEMORY TABLE LOGS(KEYA BIGINT NOT NULL,KEYB BIGINT NOT NULL,TIME BIGINT NOT NULL,XINDEX INTEGER NOT NULL,VALUE VARCHAR(255),PRIMARY KEY(KEYA,KEYB,TIME,XINDEX))
CREATE USER SA PASSWORD ""
GRANT DBA TO SA
SET WRITE_DELAY 10
